package project5b;

/**
 *
 * @author Student-HSLH133
 */
public class Person {
    private String fname = "Salvador";
    private String lname = "Salgado";
    private int age = 22;
    private String address = "123 Ocean Park Way.";
    private String newAddress;
    private int socialsecurity = 134658976;
    
    public void Person()
    {
        
    }
    
    public String getfName()
    {
        return fname;
    }
    public String getlName()
    {
        return lname;
    }
    public int getAge()
    {
        return age;
    }
    public String getAddress()
    {
        return address;
    }
    public int getSocial()
    {
        return socialsecurity;
    }
    public String setNewAdress(String newAddress)
    {
        address = newAddress;
       // System.out.print("New Address:  "+newAddress);
        return address = newAddress;
    }
    public void displayInfo()
    {
        System.out.println("First Name: " + getfName()
                             + "\nLast Name: " + getlName()
                             + "\nAge: " + getAge()
                             + "\nSocial Security: " + getSocial()
                             + "\nAddress: " + getAddress());
                         
    }
    
}
