package project5b;

public class Student extends Person{
    private int StudentID = 123456789;
    private int credits = 54;
    private double gradePointAvg = 3.7;
    private String major = "Computer Science";
    private String gradDate = "June 6 2018";
    private String university = "University of California Los Angeles";
    private String newUniversity;
    
    public int getStudentID()
    {
        return StudentID;
    }
    public int getCredits()
    {
             return credits;   
                
    }
    public double getGrade()
    {
            return gradePointAvg;    
    }
    
    public String getGradDate()
    {
        return gradDate;
    }
    public String getUniversity()
   {
       return university;
   }
    public String setNewUniversity(String newUniversity)
    {
        return university = newUniversity;
    }
    public void displayInfo()
    {
       super.displayInfo();
        System.out.println("\nStudent ID: " + getStudentID()
                             + "\nCredits Completed: " + getCredits()
                            + "\nInstitution: " + getUniversity()
                             + "\nExpected Graduation: " + getGradDate());
        
    }
}

